{
  "preset": "ts-jest",
  "extensionsToTreatAsEsm": [".ts"],
  "globals": {
      "ts-jest": {
          "useESM": true
      }
  },
  "testEnvironment": "node",
  "collectCoverage": true,
  "collectCoverageFrom": ["**/*.ts", "!**/node_modules/**"],
  "coverageReporters": ["text", "html", "cobertura"],
  "moduleNameMapper": {
    "^(\\.{1,2}/.*)\\.js$": "$1"
  }
}
