{
  "name": "mco-server",
  "version": "1.2.0",
  "description": "a server",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "test": "jest --all",
    "licence-check": "license-checker --json > ./data/licenses.json"
  },
  "author": "Joseph W Becher <jwbecher@drazisil.com> (drazisil)",
  "license": "GPL-3.0+",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:drazisil/mco-server.git"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "lib/**/*.js"
    ],
    "testResultsProcessor": "jest-junit",
    "moduleFileExtensions": [
      "js"
    ],
    "testRegex": ".*(spec|test).(js)$"
  },
  "jest-junit": {
    "output": "test-reports/junit.xml"
  },
  "dependencies": {
    "async": "^2.6.1",
    "dotenv": "^5.0.0",
    "hoek": "^5.0.3",
    "js-yaml": "^3.12.0",
    "pg": "^7.4.3",
    "require-environment-variables": "^1.1.2",
    "source-map-loader": "^0.2.3",
    "ssl-config": "^1.0.0",
    "winston": "^2.4.3"
  },
  "devDependencies": {
    "jest": "^22.4.4",
    "jest-junit": "^4.0.0",
    "license-checker": "^20.1.0",
    "pg-migrator": "^1.0.5",
    "prettier": "^1.13.5",
    "prettifier": "^0.3.5"
  },
  "prettier": {
    "trailingComma": "es5"
  }
}